<h1>students who had been graded in a subject</h1>
<div class="row">
  <div class="span6 offset3">

    <h4> select a subject</h4> 
    <%= form_for(:subject, url: studentsHadBeenGraded_path) do |f| %>
    <%= f.collection_select(:subject_id, Subject.all, :id, :name ) %>
    <br><br>

    <%= f.submit "search", class: "btn btn-primary"  %> 
    <% end %>

    <br />


<% if @users %>
<% @users.each do |user| %>
<br />
<br />
    <h3>username</h3>
    <p><%= user.name %></p>
    <h3>studentId</h3>
    <p><%= user.studentId %></p>
<br />
<% end %>
<% end %>

</div>
</div>
<%= link_to 'postFeedback', feedbackCreate_path %>
