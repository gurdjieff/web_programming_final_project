<h1>all subjects</h1>
<% @subjects.each do |subject| %>
<br />
<br />
    <h4>subject name</h4>
    <p><%= subject.name %></p>
    <h4>description</h4>
    <p><%= subject.description %></p>
    <h4>ca_percent</h4>
    <p><%= subject.ca_percent %></p>
    <h4>final_exam_percent</h4>
    <p><%= subject.final_exam_percent %></p>

    <h4>related courses</h4>
    <% subject.courses.each do |course| %>
    <p><%= link_to course.name, "#" %></p>
    <% end %>

    <h4>students that feedback the courses</h4>
    <% subject.feedbacks.each do |feedback| %>
    <p><%= link_to User.find(feedback.user_id).name, "#" %></p>

    <!-- feedback. -->
    <% end %>





 
    
    
    <br />

<% end %>
<br />

<%= link_to 'postFeedback', feedbackCreate_path %>
